{
    "name": "express-typescript-starter",
    "version": "0.1.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
    },
    "keywords": [
        "netlify",
        "express.js"
    ],
    "author": "Bowden Kelly",
    "license": "MIT",
    "main": "dist/server.js",
    "scripts": {
        "build-ts": "tsc",
        "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
        "ts-check": "tsc --noEmit --lib ES2015 ./src/*.ts",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "serve-debug": "nodemon --inspect dist/server.js",
        "serve": "node dist/server.js",
        "start": "npm run build && npm run watch-debug",
        "test": "jest --forceExit --coverage --verbose",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon dist/server.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve\""
    },
    "dependencies": {
        "@babel/plugin-syntax-flow": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-flow": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@types/aws-lambda": "^8.10.78",
        "@types/mongodb": "^3.6.5",
        "@types/socket.io": "^3.0.2",
        "async": "3.2.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "bcrypt": "^5.0.1",
        "bcrypt-nodejs": "0.0.3",
        "bcryptjs": "^2.4.3",
        "bluebird": "3.7.2",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "connect-mongo": "jdesboeufs/connect-mongo#527f9b6cd3d95d9bcd765c9ef9f2b5705611f9f5",
        "cors": "^2.8.5",
        "dotenv": "8.2.0",
        "errorhandler": "1.5.1",
        "expo-server-sdk": "^3.6.0",
        "express": "4.17.1",
        "express-flash": "0.0.2",
        "express-jwt": "^6.0.0",
        "express-session": "^1.17.1",
        "express-validator": "6.9.2",
        "fbgraph": "1.4.4",
        "jsonwebtoken": "^8.5.1",
        "lodash": "4.17.20",
        "lusca": "1.6.1",
        "moment": "^2.29.1",
        "mongoose": "5.11.15",
        "mongoose-auto-increment": "^5.0.1",
        "mongoose-sequence": "^5.3.1",
        "morgan": "^1.10.0",
        "netlify-lambda": "^2.0.10",
        "nodemailer": "6.4.17",
        "passport": "0.4.1",
        "passport-facebook": "3.0.0",
        "passport-local": "1.0.0",
        "pug": "3.0.0",
        "serverless-http": "^2.7.0",
        "socket.io": "^4.1.2",
        "socketio-jwt": "^4.6.2",
        "validate.js": "^0.13.1",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/async": "3.2.5",
        "@types/bcrypt-nodejs": "0.0.31",
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "3.5.33",
        "@types/body-parser": "1.19.0",
        "@types/chai": "4.2.14",
        "@types/compression": "1.7.0",
        "@types/concurrently": "5.2.1",
        "@types/cors": "^2.8.10",
        "@types/errorhandler": "1.5.0",
        "@types/eslint": "7.2.6",
        "@types/express": "4.17.11",
        "@types/express-flash": "0.0.2",
        "@types/express-jwt": "^6.0.1",
        "@types/express-session": "1.17.3",
        "@types/jest": "26.0.20",
        "@types/jquery": "3.5.5",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/lodash": "4.14.168",
        "@types/lusca": "1.6.2",
        "@types/mongoose-sequence": "^3.0.6",
        "@types/morgan": "^1.9.2",
        "@types/node": "14.14.25",
        "@types/nodemailer": "6.4.0",
        "@types/passport": "1.0.5",
        "@types/passport-facebook": "2.1.10",
        "@types/passport-local": "1.0.33",
        "@types/pug": "2.0.4",
        "@types/request": "2.48.5",
        "@types/request-promise": "4.1.47",
        "@types/shelljs": "0.8.8",
        "@types/supertest": "2.0.10",
        "@types/uuid": "^8.3.0",
        "@types/winston": "2.4.4",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "chai": "4.3.0",
        "concurrently": "5.3.0",
        "eslint": "7.19.0",
        "jest": "26.6.3",
        "nodemon": "2.0.7",
        "sass": "1.32.6",
        "shelljs": "0.8.4",
        "supertest": "6.1.3",
        "ts-jest": "26.5.0",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    }
}
